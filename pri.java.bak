class pri
{
	public static void main(String[]args)
	{
		int num=20;
		int count=0;
		for(int i=2; i<=20; i++)
		{
			while(num>=i)
			{
			if(num%i==0)
			{
				break;
			}
			i++;
		
			if(num==i)
			{
				System.out.println("prime number " +num);
					count++;
			}
			     
				 
			else
			{
				System.out.println("not a prime number " +num);
			}
			
		}
		num--;
		
	}
	
}
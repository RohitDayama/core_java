{
int  a [] ={10,20,30,40,50};
int sum=0;
int prod=01;
for(int i=0; i<a.length; i++)
{
	if (a[i]%2==0)
	{
		sum=sum+a[i];
	}
    else
    {
     prod=prod*a[i];	
    }
}

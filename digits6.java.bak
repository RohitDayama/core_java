class digits
{
public static void main(String [] args)
{
	int num=123456;
	String ev= " ";
	String od=" ";
	while(num>0)
	{
		int dig=num%10;
		if (dig%2==0)
		{
			ev=dig+ev;
		}
		else 
		{
			od=dig+od;
		}
num=num/10;
	}
System.out.println("even no" + ev);
System.out.println("odd no" + od);
}
}	